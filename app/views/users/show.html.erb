<% provide(:title, @user.email) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
       <h1>
         <%= gravatar_for @user %>
         <%= @user.name %>
      </h1>
    </section>
  </aside>

  <div class="col-md-7">
  
 
  <h1>My Blogs</h1>
  <ul class="posts">
    <%= render @albums %>
  </ul>
  </div>
</div>
